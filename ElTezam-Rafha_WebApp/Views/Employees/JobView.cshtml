@{
    ViewData["Title"] = "Home Page";
    Layout = "/Views/Shared/DataTableLayout.cshtml";
}
<div class="card-body">
    <h4 class="card-title">ادخال موظفين</h4>
    <div class="row">
        <div class=" col-lg-4">
            <input type="file" id="sheet" class="form-control-file"><br />
            <input type="submit" id="upload" onclick="SubmitSheet()" class="form-control btn btn-info" value="تحميل الي قاعدة البيانات" />

        </div>
        <div class=" col-lg-4">
            <input type="submit" id="uploadtoservice" onclick="sendToService()" class="form-control btn btn-info" value="ارسال للجهة">
            <input type="submit" id="uploadtoservice" onclick="sendAllDataToService()" class="form-control btn btn-info" value="ارسال جميع البيانات للجهة">
            <input type="submit" onclick="BulkDelete()" class="form-control btn btn-danger" value=" حذف مجمع او مفرد">
        </div>
    </div>
</div>
<table class="table table-striped table-bordered " id="employeeData">
    <thead>
        <tr>
            <th>الاسم</th>
            <th>رقم الوظيفة</th>
            <th>رمز سلسلة الفئات الوظيفية </th>
            <th>رمز مسمى الوظيفة</th>
            <th>رمز المرتبة</th>
            <th>تاريخ بداية الإجراء</th>
            <th></th>
        </tr>
    </thead>
    <tbody></tbody>
    <tfoot>
        <tr>
            <th>الاسم</th>
            <th>رقم الوظيفة</th>
            <th>رمز سلسلة الفئات الوظيفية </th>
            <th>رمز مسمى الوظيفة</th>
            <th>رمز المرتبة</th>
            <th>تاريخ بداية الإجراء</th>
            <th></th>
        </tr>
    </tfoot>
</table>
@section Scripts{
    <script src="~/viewsscripts/JobView.js"></script>
}