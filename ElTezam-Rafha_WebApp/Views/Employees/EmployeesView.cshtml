@{
    ViewData["Title"] = "Home Page";
    Layout = "/Views/Shared/DataTableLayout.cshtml";
}
<div class="card-body">
    <h4 class="card-title data-title">إرسال البيانات لخدمة إلتزام</h4>
    <div class="row ">
        <div class=" col-lg-12 mb-lg-5 ">
            <input type="file" id="sheet" class="form-control-file">
        </div>
        <div class="col-lg-8 d-flex justify-content">
            <input type="submit" id="upload" onclick="SubmitSheet()" class="form-control btn btn-download" value="رفع البيانات" />
            <input type="submit" id="uploadtoservice" onclick="sendToService()" class="form-control btn btn-sent" value="إرسال للجهة">
            <input type="submit" id="uploadAllDatatoservice" onclick="sendAllDataToService()" class="form-control btn btn-sent" value="إرسال جميع البيانات للجهة">
            <input type="submit" onclick="BulkDelete()" class="form-control btn btn-danger" value=" حذف البيانات">
        </div>
        <div class="">
        </div>
        <div class="">
        </div>
    </div>
</div>

<table class="table table-striped table-bordered " id="employeeData">
    <thead>
        <tr>
            <th>الاسم</th>
            <th>تاريخ الميلاد</th>
            <th>الجنسية</th>
            <th>الوظيفة</th>
            <th>المرتبة</th>
            <th> الإجراء الوظيفي</th>
            <th> </th>
        </tr>
    </thead>
    <tbody id="employees"></tbody>
</table>
@section Scripts{
    <script src="~/viewsscripts/employeesview.js"></script>
}